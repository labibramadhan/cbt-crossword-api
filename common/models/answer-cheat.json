{
  "name": "AnswerCheat",
  "plural": "AnswerCheats",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "created_at",
      "updatedAt": "updated_at"
    },
    "CreatedBy": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "postgresql": {
        "dataType": "uuid",
        "dbDefault": "uuid_generate_v4()"
      }
    },
    "duration": {
      "type": "number",
      "required": true
    },
    "sanction": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "answer": {
      "type": "belongsTo",
      "model": "answer",
      "foreignKey": "answer_id"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "participant",
      "permission": "ALLOW",
      "property": "*"
    },
    {
      "principalType": "ROLE",
      "principalId": "participant",
      "permission": "ALLOW",
      "property": "create"
    }
  ],
  "methods": {}
}
